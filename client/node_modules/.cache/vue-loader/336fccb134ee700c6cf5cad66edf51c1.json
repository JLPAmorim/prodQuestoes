le: '',\r\n              notes: '',\r\n              access_type: '',\r\n              body: [],\r\n              default_user_level: '',\r\n              high_performance_factor: '',\r\n              low_performance_factor: '',\r\n              high_skill_factor: '',\r\n              low_skill_factor: '',\r\n              min_questions_number: '',\r\n              question_factor: '',\r\n              inserted_by: \"User_default\",\r\n              inserted_at:new Date().toLocaleString()\r\n            },\r\n            search:'',\r\n            navDomains: [],\r\n        }\r\n    },\r\n    created(){\r\n        axios.get(`http://localhost:8001/domain`)\r\n          .then((response)=>{\r\n            this.navDomains=response.data\r\n          },(error) =>{\r\n              console.log(error);\r\n          });\r\n    },\r\n    methods: {\r\n\r\n      validate() {\r\n        return this.$refs.form.validate()\r\n      },\r\n      \r\n      showItem (item) {\r\n        this.itemIndex = this.navDomains.indexOf(item)\r\n        this.domain = Object.assign({}, item)\r\n        this.dialogShow = true\r\n      },\r\n\r\n      sendItem(data){\r\n        this.$router.push({\r\n          name: \"ProdDominio\", \r\n          params: { data }\r\n        });\r\n      },\r\n\r\n      closeShow(){\r\n        this.dialogShow = false\r\n      },\r\n\r\n      deleteItem(item){\r\n        this.itemIndex = this.navDomains.indexOf(item)\r\n        this.domain = Object.assign({}, item)\r\n        this.dialogDelete = true\r\n      },     \r\n\r\n      deleteConfirm(){\r\n        axios.delete(`http://localhost:8001/domain/`+this.domain.id)\r\n          .then((response)=>{\r\n            console.log(response.data)\r\n            console.log(this.itemIndex)\r\n            this.navDomains.splice(this.itemIndex, 1)\r\n          },(error) =>{\r\n              console.log(error);\r\n          });\r\n        this.closeDelete()\r\n      },\r\n\r\n      closeDelete(){\r\n        this.dialogDelete = false\r\n      }\r\n    },\r\n\r\n}\r\n</script>\r\n<style scoped>\r\n    /* css para tornar a aparência mais similar ao template */\r\n    .v-data-table /deep/ th{\r\n        background-color:#4b779e;\r\n        ;\r\n    }\r\n    .v-data-table /deep/ tr{\r\n        color: #73879C;\r\n        font-size: 13px;\r\n    }\r\n    .v-data-table /deep/ tr:nth-child(even){\r\n        background-color: rgb(245, 245, 245);\r\n    }\r\n\r\n    a { \r\n      text-decoration: none; \r\n    }\r\n\r\n    .title {\r\n      color: #2A3F54\r\n    }\r\n\r\n\r\n</style>"]}]}