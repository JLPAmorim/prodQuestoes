A,CAAA,EAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,EAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,EAAA,CAAA,KAAA;AACA,KApCA;AAsCA,IAAA,UAtCA,wBAsCA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAxCA;AA0CA,IAAA,WA1CA,yBA0CA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KA5CA;AA8CA,IAAA,aA9CA,2BA8CA;AAAA;;AACA,UAAA,KAAA,QAAA,IAAA,EAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,oCAAA,KAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,EAAA,GAAA,QAAA,CAAA,IAAA,CAAA,EAAA,EACA,KAAA,CAAA,OAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,QADA,EAEA,KAAA,CAAA,OAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAFA,EAGA,KAAA,CAAA,OAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,UAHA,EAIA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAJA,EAKA,KAAA,CAAA,OAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SALA,EAMA,KAAA,CAAA,OAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CAAA,YANA,EAOA,KAAA,CAAA,OAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,gBAPA,EAQA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MARA,EASA,KAAA,CAAA,OAAA,CAAA,YAAA,GAAA,QAAA,CAAA,IAAA,CAAA,EATA,EAUA,KAAA,CAAA,OAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,cAVA,EAWA,KAAA,CAAA,OAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAXA,EAYA,KAAA,CAAA,OAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,UAZA,EAaA,KAAA,CAAA,OAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAbA;AAcA,UAAA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,EACA,KAAA,CAAA,OAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IADA,EAEA,KAAA,CAAA,OAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,WAFA,EAGA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAHA,EAIA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAJA,EAKA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MALA,EAMA,KAAA,CAAA,OAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KANA,EAOA,KAAA,CAAA,OAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAPA;AAQA,UAAA,OAAA,CAAA,EAAA;AACA,SA1BA,EA0BA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SA5BA;AA8BA,aAAA,UAAA,GAAA,KAAA;AACA,OAjCA,MAiCA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA;AAEA,KApFA;AAsFA,IAAA,WAtFA,yBAsFA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KAxFA;AA0FA,IAAA,IA1FA,kBA0FA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA5FA;AA8FA,IAAA,SA9FA,uBA8FA;AACA,WAAA,QAAA,GAAA,KAAA;AACA;AAhGA;AAtDA,CAAA","sourcesContent":["<template>   \n     <v-container class=\"my-5\">\n       <AppHeader></AppHeader>\n        <div>\n            <NavDraw></NavDraw>\n        </div>\n           <v-layout justify-center row wrap>\n             <v-flex xs10 sm10 md9 lg9>\n                <v-card color=\"#2A3F54\" elevation=\"10\">\n                  <v-row >\n                    <v-col cols=\"2\" lg=\"2\" xl=\"1\">\n                      <v-img src=\"@/assets/raccoon.png\" max-width=\"80\" class=\"mt-5 ml-5\"></v-img>\n                    </v-col>\n                    <v-col>\n                      <v-card-title class=\"yellow--text text--darken-2 text-h4 mr-5\">Produção</v-card-title>\n                      <v-card-subtitle class=\"white--text text-h5\">Preparação de Questões</v-card-subtitle>\n                    </v-col>\n                  </v-row>\n                </v-card>\n\n                <v-card class=\"mt-2\" elevation=\"10\" >\n                  <v-tabs v-model=\"tab\"  show-arrows>\n                    <v-tab v-for=\"item in items\" :key=\"item.tab\">\n                     {{ item.tab }}\n                    </v-tab>\n                  </v-tabs>\n                  <v-tabs-items v-model=\"tab\">\n                   <v-tab-item eager> \n                      <Caracterizacao ref=\"ct\" @newdataCaracterizacao=\"handleDataCaracterizacao($event)\"/>\n                   </v-tab-item>\n                   <v-tab-item eager>\n                      <Respostas ref=\"rp\" @newdataRespostas=\"handleDataRespostas($event)\"/>\n                   </v-tab-item>\n                   <v-tab-item eager>\n                      <Suporte ref=\"sp\" @newdataSuporte=\"handleDataSuporte($event)\"/>\n                   </v-tab-item>\n                 </v-tabs-items>\n              </v-card>\n              \n              <v-card class=\"mx-auto\" >\n                <v-row class=\"px-2 pb-2 ma-0 py-2\" justify=\"space-between\">\n                  <v-btn-toggle v-model=\"formatting\" multiple dense class=\"ml-5 mb-3\">\n                    <v-btn @click=\"confirmSubmit\" color=\"#F0B62B\" elevation=\"5\" class=\"mr-2\">\n                      <v-icon color=\"white\">mdi-checkbox-marked-outline</v-icon>\n                    </v-btn>\n\n                    <v-btn color=\"white\" elevation=\"5\" class=\"mr-2\" @click=\"startImport\">\n                      <v-icon color=\"black\">mdi-import</v-icon>\n                    </v-btn>\n\n                    <v-btn color=\"#29E898\" elevation=\"5\" @click=\"reset\">\n                      <v-icon color=\"white\">mdi-broom</v-icon>\n                    </v-btn>\n                  </v-btn-toggle>\n\n                  <v-btn-toggle v-model=\"alignment\"\n                    dense class=\"mr-5 mb-3\">\n                    <v-btn color=\"#2A3F54\" class=\"mr-2\" elevation=\"5\" @click=\"help\">\n                      <v-icon color=\"white\">mdi-help</v-icon>\n                    </v-btn>\n                    <v-btn to=\"/questoes\" color=\"#29E898\" elevation=\"5\" class=\"\">\n                      <v-icon color=\"white\">mdi-door-open</v-icon>\n                    </v-btn>\n                  </v-btn-toggle>\n                </v-row>\n                <!-- Janela para Confirmação da Submissão -->\n                <v-dialog v-model=\"openSubmit\" max-width=\"500px\">\n                  <v-card>\n                    <v-app-bar color=\"#2A3F54\" >\n                      <div class=\"d-flex align-center\">\n                        <h3 width=\"40\" class=\"white--text\"> Confirmar Submissão</h3>\n                      </div>\n                    </v-app-bar>\n                    <v-container>\n                      <v-row>\n                        <v-col cols=\"3\">\n                          <v-card class=\"ml-4 mt-1\" color=\"white\" flat height=\"100px\" width=\"110px\" >\n                              <v-img src=\"@/assets/questionmark.png\"/>\n                          </v-card>\n                        </v-col>\n                        <v-col cols=\"9\">\n                          <h3 class=\"ml-5 mt-5\">Pretende confirmar a submissão da questão?</h3>\n                        </v-col>\n                      </v-row>\n                    </v-container>\n                    <v-card-actions>\n                      <v-container>\n                          <v-row >\n                            <v-col class=\"text-right\">\n                              <v-btn color=\"#F0B62B\" @click=\"submit\" elevation=\"5\" class=\"mt-5 mr-3\">\n                                <v-icon color=\"white\">mdi-checkbox-marked-outline</v-icon>\n                              </v-btn>\n                              <v-btn color=\"#29E898\" @click=\"closeDelete\" elevation=\"5\" class=\"mt-5\">\n                                <v-icon color=\"white\">mdi-door-open</v-icon>\n                              </v-btn>\n                            </v-col>\n                          </v-row>\n                      </v-container>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <!-- Janela de Erro na Submissão -->\n                <v-dialog v-model=\"openError\" max-width=\"500px\">\n                  <v-card>\n                    <v-app-bar color=\"#2A3F54\" >\n                      <div class=\"d-flex align-center\">\n                        <h3 width=\"40\" class=\"white--text\"> Erro na Submissão</h3>\n                      </div>\n                    </v-app-bar>\n                    <v-container>\n                      <v-row>\n                        <v-col cols=\"3\">\n                          <v-card class=\"ml-4 mt-1\" color=\"white\" flat height=\"100px\" width=\"110px\" >\n                              <v-img src=\"@/assets/error.png\"/>\n                          </v-card>\n                        </v-col>\n                        <v-col cols=\"9\">\n                          <h3 class=\"ml-5 mt-5\">Erro na submissão da questão!</h3>\n                          <h3 class=\"ml-5\">Por favor preencha todos os campos obrigatórios.</h3>\n                        </v-col>\n                      </v-row>\n                    </v-container>\n                    <v-card-actions>\n                      <v-container>\n                          <v-row >\n                            <v-col class=\"text-right\">\n                              <v-btn color=\"#29E898\" @click=\"closeError\" elevation=\"5\" class=\"mt-5\">\n                                <v-icon color=\"white\">mdi-door-open</v-icon>\n                              </v-btn>\n                            </v-col>\n                          </v-row>\n                      </v-container>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <!-- Janela de Import -->\n                <v-dialog v-model=\"openImport\" max-width=\"500px\">\n                  <v-card>\n                    <v-app-bar color=\"#2A3F54\" >\n                      <div class=\"d-flex align-center\">\n                        <h3 width=\"40\" class=\"white--text\"> Import de Questão</h3>\n                      </div>\n                    </v-app-bar>\n                    <v-container>\n                      <v-row>\n                        <v-col cols=\"9\">\n                          <h3 class=\"ml-5 mt-5\">Insira o identificador da questão que pretende importar.</h3>\n                        </v-col>\n                      </v-row>\n                      <v-row>\n                        <v-col cols=\"9\" class=\"ml-5\">\n                          <v-text-field v-model=\"idImport\" \n                            :rules=\"required\" \n                            label=\"Identificador\"/>\n                        </v-col>\n                      </v-row>\n                    </v-container>\n                    <v-card-actions>\n                      <v-container>\n                          <v-row >\n                            <v-col class=\"text-right\">\n                              <v-btn color=\"#F0B62B\" @click=\"confirmImport\" elevation=\"5\" class=\"mt-5 mr-3\">\n                                <v-icon color=\"white\">mdi-import</v-icon>\n                              </v-btn>\n                              <v-btn color=\"#29E898\" @click=\"closeImport\" elevation=\"5\" class=\"mt-5\">\n                                <v-icon color=\"white\">mdi-door-open</v-icon>\n                              </v-btn>\n                            </v-col>\n                          </v-row>\n                      </v-container>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n                <!-- Janela de Diálogo para Ajuda -->\n                <v-dialog v-model=\"openHelp\" max-width=\"500px\">\n                  <v-card>\n                    <v-app-bar color=\"#2A3F54\" >\n                      <div class=\"d-flex align-center\">\n                        <h3 width=\"40\" class=\"white--text\"> Ajuda</h3>\n                      </div>\n                    </v-app-bar>\n                    <v-container>\n                      <v-row>\n                        <v-col cols=\"3\">\n                          <v-card class=\"ml-4 mt-1\" color=\"white\" flat height=\"100px\" width=\"110px\" >\n                              <v-img src=\"@/assets/information.png\"/>\n                          </v-card>\n                        </v-col>\n                        <v-col cols=\"9\">\n                          <h3 class=\"ml-5 mt-5\">Preencha todos os campos que constituem a questão.</h3>\n                          <h3 class=\"ml-5\">Navegue por todas as tabs para garantir que toda a informação se encontra correta.</h3>\n                        </v-col>\n                      </v-row>\n                    </v-container>\n                    <v-card-actions>\n                      <v-container>\n                          <v-row >\n                            <v-col class=\"text-right\">\n                              <v-btn color=\"#29E898\" @click=\"closeHelp\" elevation=\"5\" class=\"mt-5\">\n                                <v-icon color=\"white\">mdi-door-open</v-icon>\n                              </v-btn>\n                            </v-col>\n                          </v-row>\n                      </v-container>\n                    </v-card-actions>\n                  </v-card>\n                </v-dialog>\n              </v-card>\n\n             </v-flex>\n           </v-layout>   \n           <Footer class=\"mt-5\"></Footer>   \n     </v-container>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Caracterizacao from '@/components/Caracterizacao'\nimport Respostas from '@/components/Respostas'\nimport Suporte from '@/components/Suporte'\nimport AppHeader from '@/components/AppHeader'\nimport NavDraw from '@/components/NavDraw'\nimport Footer from '@/components/Footer'\n\nexport default {\n  components: { \n        Caracterizacao, \n        Respostas,\n        Suporte,\n        AppHeader,\n        NavDraw,\n        Footer\n    }, \n  data() {\n    return{\n      tab: null,\n      openHelp: false,\n      openImport: false,\n      openSubmit: false,\n      openError: false,\n      idImport: '',\n      required: [(v) => !!v || \"Field is required\"],\n      items: [\n        { tab: 'Caracterizacão'},\n        { tab: 'Respostas'},\n        { tab: 'Suporte'}\n      ],\n      questao:{\n        id: '',\n        language: \"pt\", \n        study_cycle: '',\n        scholarity: '',\n        domain: '',\n        subdomain: '',\n        subsubdomain: '',\n        difficulty_level: '',\n        author: '',\n        display_mode: '', \n        answering_time: '',\n        type: '',\n        precedence: [], \n        repetitions: '',\n        header:'',\n        body: [], \n        explanation: \"\", \n        images: \"\", \n        videos: \"\", \n        source: \"\", \n        notes: \"\", \n        status:\"E\", \n        inserted_by: \"User_default\", \n        inserted_at:new Date().toLocaleString(), \n        validated_by:\"\", \n        validated_at:\"\" \n      }\n    }\n  },\n \n  methods:{\n\n    handleDataCaracterizacao(e) {\n      [this.questao.id,this.questao.study_cycle,this.questao.scholarity,this.questao.domain,this.questao.subdomain,\n      this.questao.subsubdomain, this.questao.header, this.questao.difficulty_level,this.questao.author,this.questao.display_mode,\n      this.questao.answering_time,this.questao.type,this.questao.precedence,this.questao.repetitions] = e;\n    },\n\n    handleDataRespostas(e) {\n      this.questao.body = e;\n    },\n\n    handleDataSuporte(e) {\n      [this.questao.explanation,this.questao.notes,this.questao.source,this.questao.status,this.questao.language] = e;\n    },\n\n    confirmSubmit(){\n      if(this.$refs.ct.validate() && this.$refs.rp.validate() && this.$refs.sp.validate()){\n        this.openSubmit = true\n      }\n      else{\n        this.openError = true   \n      } \n    },\n\n    submit(){\n        axios.post(`http://localhost:8001/question`, this.questao)\n          .then(function(response){\n            console.log(response)\n          }); \n    },\n\n    reset() {\n      this.$refs.ct.reset()\n      this.$refs.rp.reset()\n      this.$refs.sp.reset()\n    },\n\n    closeError() {\n      this.openError = false\n    },\n\n    startImport() {\n      this.openImport = true\n    },\n\n    confirmImport(){\n      if(this.idImport!=''){\n        console.log(this.idImport)\n        axios.get(`http://localhost:8001/question/`+this.idImport)\n          .then((response)=>{\n            console.log(response.data.id)\n            this.questao.id = response.data.id,\n            this.questao.language = response.data.language, \n            this.questao.study_cycle = response.data.study_cycle,\n            this.questao.scholarity = response.data.scholarity,\n            this.questao.domain = response.data.domain,\n            this.questao.subdomain = response.data.subdomain,\n            this.questao.subsubdomain = response.data.subsubdomain,\n            this.questao.difficulty_level = response.data.difficulty_level,\n            this.questao.author = response.data.author,\n            this.questao.display_mode = response.data.id, \n            this.questao.answering_time = response.data.answering_time,\n            this.questao.type = response.data.type,\n            this.questao.precedence = response.data.precedence,\n            this.questao.repetitions = response.data.repetitions\n            this.questao.header = response.data.header,\n            this.questao.body = response.data.body,\n            this.questao.explanation = response.data.explanation,\n            this.questao.images = response.data.images,\n            this.questao.videos = response.data.videos,\n            this.questao.source = response.data.source,\n            this.questao.notes = response.data.notes,\n            this.questao.status = response.data.status\n            $router.go()\n          },(error) =>{\n              console.log(error);\n        });\n        \n        this.openImport = false\n      }else{\n        console.log(this.idImport)\n      }\n      \n    },\n\n    closeImport() {\n      this.openImport = false\n    },\n\n    help() {\n      this.openHelp = true\n    },\n\n    closeHelp() {\n      this.openHelp = false\n    },\n\n    \n  }\n}\n</script>\n\n\n\n"],"sourceRoot":"src/views"}]}